pipeline {
    agent any

    stages {
        stage('Local-Build Dev Container') {
            steps {
                echo 'Starting to build docker image'

                script {
                    def dev_image = docker.build("devcontainer:latest", "-f devops/devcontainer/Dockerfile .")
                    // customImage.push()
                }
            }
        }
    }
    post {
		always {
            deleteDir()
		}
    }
}